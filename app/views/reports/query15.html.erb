<div class="container">
    <div class="col-md-6">
        <h3>Members with the highest and the least number of rentals</h3>
    </div>
    <table class="table table-bordered table-striped table-condensed table-hover report-table">
        <thead>
            <div class="row">
                <tr>
                <th>Name</th>
                <th>Position</th>
                </tr>
            </div>
        </thead>
    <tbody>
        <div class="row">
            <% @vquery.each do |p| %>
                <tr>
                <td><%= p['Nombre'] %></td>
                <td><%= p['Position'] %></td>                
                </tr>
            <% end %>
        </div>
    </tbody>
    </table>
</div>